+    # Lancer l'arr√™t dans un thread s√©par√©
+    import threading
+    threading.Thread(target=stop_server, daemon=True).start()
+    
+    return {
+        "status": "shutdown_initiated",
+        "message": "Le serveur s'arr√™te dans quelques instants..."
+    }
+
