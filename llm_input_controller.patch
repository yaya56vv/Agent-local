+            return {
+                "status": "success",
+                "message": f"Copied {src.name} to {dest}",
+                "source": str(src),
+                "destination": str(dest)
+            }
+            
+        except Exception as e:
+            return {
+                "status": "error",
+                "error": str(e),
+                "source": src_path,
+                "destination": dest_path
+            }
