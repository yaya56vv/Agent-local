+"""
+Input Controller
+~~~~~~~~~~~~~~~~
+
+This module exposes a simple `InputController` class that provides a unified
+interface for basic mouse and keyboard automation.  It relies on the
+`pyautogui` library, which offers cross‚Äëplatform GUI automation, to move
+the mouse, click, scroll and type/press keys.  Each method accepts an
+`allow` argument; if set to ``False`` (the default), the action will not
+be executed and an error is returned.  This pattern enforces explicit
+permission for potentially sensitive actions.
+
+The orchestrator calls these methods when executing steps such as
+``mouse_move`` or ``keyboard_type``.  The return value is a dictionary
+indicating success or error along with any relevant details.
+"""
+
+from __future__ import annotations
+
+from typing import List, Optional, Dict, Any
+
+import pyautogui
+
+
+class InputController:
+    """Controller for mouse and keyboard actions.
+
+    This class wraps common GUI automation primitives from :mod:`pyautogui`
+    behind an interface expected by the orchestrator.  It enforces a
+    permission check via the ``allow`` parameter on each method.
+    """
+
+    def __init__(self) -> None:
+        # Configure pyautogui for safety: moving the mouse to a corner
+        # aborts any ongoing operation, and a short pause between actions
+        # prevents overwhelming the UI.
+        pyautogui.FAILSAFE = True
+        pyautogui.PAUSE = 0.1
+
+    def mouse_move(self, x: int, y: int, duration: float = 0.5, allow: bool = False) -> Dict[str, Any]:
+        """Move the mouse to an absolute position.
+
+        Parameters
+        ----------
+        x: int
+            The target X coordinate (pixels).
+        y: int
+            The target Y coordinate (pixels).
+        duration: float, optional
+            Time in seconds over which to move the cursor.
+        allow: bool, optional
+            Must be ``True`` to execute the movement; otherwise an error is returned.
+
+        Returns
+        -------
+        dict
+            A result dictionary with ``status`` (``"success"`` or ``"error"``) and
+            coordinates if successful.
+        """
+        if not allow:
+            return {"status": "error", "error": "Permission denied"}
+        try:
+            pyautogui.moveTo(x, y, duration=duration)
+            return {"status": "success", "x": x, "y": y}
+        except Exception as e:
+            return {"status": "error", "error": str(e)}
+
+    def mouse_click(
+        self,
+        x: Optional[int] = None,
+        y: Optional[int] = None,
+        button: str = "left",
+        clicks: int = 1,
+        allow: bool = False,
+    ) -> Dict[str, Any]:
+        """Click the mouse at the specified position.
+
+        If coordinates ``x`` and ``y`` are provided, the mouse is moved there
+        first.  Otherwise it clicks at the current position.  The button
+        (``"left"``, ``"right"`` or ``"middle"``) and number of clicks
+        can be specified.
+        """
+        if not allow:
+            return {"status": "error", "error": "Permission denied"}
+        try:
+            if x is not None and y is not None:
+                pyautogui.moveTo(x, y)
+            pyautogui.click(x=x, y=y, clicks=clicks, button=button)
+            return {"status": "success", "x": x, "y": y, "button": button, "clicks": clicks}
+        except Exception as e:
+            return {"status": "error", "error": str(e)}
+
+    def mouse_scroll(self, clicks: int, allow: bool = False) -> Dict[str, Any]:
+        """Scroll the mouse wheel.
+
+        Parameters
+        ----------
+        clicks: int
+            Number of scroll units.  Positive values scroll up; negative scroll down.
+        allow: bool, optional
+            Must be ``True`` to execute.
+        """
+        if not allow:
+            return {"status": "error", "error": "Permission denied"}
+        try:
+            # Multiply by 100 to make the scroll more noticeable; adjust as needed.
+            pyautogui.scroll(clicks * 100)
+            return {"status": "success", "scroll_delta": clicks}
+        except Exception as e:
+            return {"status": "error", "error": str(e)}
+
+    def keyboard_type(self, text: str, interval: float = 0.05, allow: bool = False) -> Dict[str, Any]:
+        """Type a string of text using the keyboard.
+
+        Parameters
+        ----------
+        text: str
+            The text to type.
+        interval: float, optional
+            Pause between keystrokes in seconds.
+        allow: bool, optional
+            Must be ``True`` to perform the typing.
+        """
+        if not allow:
+            return {"status": "error", "error": "Permission denied"}
+        try:
+            pyautogui.write(text, interval=interval)
+            return {"status": "success", "text": text}
+        except Exception as e:
+            return {"status": "error", "error": str(e)}
+
+    def keyboard_press(self, keys: List[str], allow: bool = False) -> Dict[str, Any]:
+        """Press one or more keys.
+
+        When a single key is provided, this method uses :func:`pyautogui.press`.
+        For multiple keys (e.g. shortcuts), it uses :func:`pyautogui.hotkey`.
+        """
+        if not allow:
+            return {"status": "error", "error": "Permission denied"}
+        try:
+            if not keys:
+                return {"status": "error", "error": "No keys provided"}
+            if len(keys) == 1:
+                pyautogui.press(keys[0])
+            else:
+                # pyautogui.hotkey automatically handles key down/up in order
+                pyautogui.hotkey(*keys)
+            return {"status": "success", "keys": keys}
+        except Exception as e:
+            return {"status": "error", "error": str(e)}
