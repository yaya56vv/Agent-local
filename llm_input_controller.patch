+                elif "summary" in desc or "overview" in desc:
+                    info["operation"] = "summary"
+                else:
+                    info["operation"] = "recall"
+                
+                # Try to extract session_id if mentioned
+                if "session" in desc:
+                    parts = desc.split("session")
+                    if len(parts) > 1:
+                        session_part = parts[1].strip().split()[0]
+                        if session_part:
