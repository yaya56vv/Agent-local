+"""
+LLM Router
+~~~~~~~~~~~
+
+This module provides a simple routing function to select an appropriate
+large language model (LLM) based on the type of task being executed and
+whether the request includes multimodal input (e.g. an image).  It
+consults the central configuration defined in :mod:`backend.config.settings`
+to decide which model to use and returns a dictionary with the chosen
+model, the name of the specialist, and a human‚Äëreadable reason.  If a
+specialised model is disabled via the settings, the router falls back to
+the default or reasoning model.
+
+The orchestrator uses this helper to log which specialist is being used
+for each step of a plan.  The actual LLM instances are created in the
+:class:`~backend.orchestrator.orchestrator.Orchestrator` using the
+settings; this router only selects the appropriate model name.
+"""
+
+from __future__ import annotations
+
+from typing import Dict
+
+from backend.config.settings import settings
+
+
+def pick_model(task: str, multimodal: bool = False) -> Dict[str, str]:
+    """Select the most appropriate LLM model for a given task.
+
+    The selection logic is based on the task type (e.g. ``"code_analyze"``,
+    ``"vision_analysis"``) and the presence of multimodal input.  For
+    specialised tasks (vision, code, conversation, RAG) the corresponding
+    model is chosen if the specialist is enabled.  Otherwise the router
+    falls back to the default or reasoning model.
+
+    Parameters
+    ----------
+    task: str
+        Name of the task or intent.  This may be an action name from the
+        orchestrator's plan (e.g. ``"code_analyze"``) or a high‚Äëlevel
+        intent like ``"vision_analysis"`` or ``"conversation"``.
+    multimodal: bool, optional
+        If ``True``, indicates that the current request includes image data
+        (screenshots or pictures) and should favour the vision model.
+
+    Returns
+    -------
+    Dict[str, str]
+        A dictionary containing the selected ``model``, the ``specialist``
+        describing why this model was chosen, and a ``reason`` explaining
+        the decision.  Additional keys may be added in the future.
+    """
+    # Normalise the task name for comparison
+    task_lower = (task or "").lower()
+
+    # Prioritise vision for multimodal tasks or explicit vision actions
+    vision_tasks = {
+        "vision_analysis",
+        "image_analysis",
+        "screenshot_analysis",
+        "vision",
+    }
+    if multimodal or task_lower in vision_tasks:
+        if settings.LLM_ENABLE_VISION:
+            return {
+                "model": settings.LLM_VISION_MODEL,
+                "specialist": "vision",
+                "reason": "Vision or multimodal task",
+            }
+        # Fallback when vision specialist is disabled
+        return {
+            "model": settings.LLM_DEFAULT_MODEL,
+            "specialist": "default",
+            "reason": "Vision specialist disabled; using default model",
+        }
+
+    # Code‚Äërelated tasks
+    code_tasks = {
+        "code_execution",
+        "code_analyze",
+        "code_explain",
+        "code_optimize",
+        "code_debug",
+        "code",
+        "coding",
+    }
+    if task_lower in code_tasks:
+        if settings.LLM_ENABLE_CODE:
+            return {
+                "model": settings.LLM_CODE_MODEL,
+                "specialist": "code",
+                "reason": "Coding task",
+            }
+        return {
+            "model": settings.LLM_DEFAULT_MODEL,
+            "specialist": "default",
+            "reason": "Code specialist disabled; using default model",
+        }
+
+    # RAG tasks (knowledge retrieval/addition)
+    rag_tasks = {
+        "rag_query",
+        "rag_add",
+    }
+    if task_lower in rag_tasks:
+        if settings.LLM_ENABLE_RAG:
+            return {
+                "model": settings.LLM_RAG_MODEL,
+                "specialist": "rag",
+                "reason": "RAG knowledge task",
+            }
+        return {
+            "model": settings.LLM_DEFAULT_MODEL,
+            "specialist": "default",
+            "reason": "RAG specialist disabled; using default model",
+        }
+
+    # Conversation / small talk
+    conversation_tasks = {
+        "conversation",
+        "chat",
+        "greeting",
+        "hello",
+        "small talk",
+    }
+    if task_lower in conversation_tasks:
+        if settings.LLM_ENABLE_CONVERSATION:
+            return {
+                "model": settings.LLM_CONVERSATION_MODEL,
+                "specialist": "conversation",
+                "reason": "General conversation task",
+            }
+        return {
+            "model": settings.LLM_DEFAULT_MODEL,
+            "specialist": "default",
+            "reason": "Conversation specialist disabled; using default model",
+        }
+
+    # Memory tasks fall under reasoning model (e.g. recall, search)
+    if task_lower.startswith("memory"):
+        return {
+            "model": settings.LLM_REASONING_MODEL,
+            "specialist": "reasoning",
+            "reason": "Memory operation uses reasoning model",
+        }
+
+    # For any unrecognised or general task, use reasoning model
+    return {
+        "model": settings.LLM_REASONING_MODEL,
+        "specialist": "reasoning",
+        "reason": "Default reasoning for unclassified task",
+    }
